{
    "name" : "manuscle/such-table",
    "description" : "Zend Framework 2 Module for generating HTML tables",
    "type" : "module",
    "license" : "BSD-3-Clause",
    "homepage" : "https://github.com/Manuscle/SuchTable",
    "keywords" : [
        "table",
        "html table",
        "Zend Framework",
        "zf2",
        "module"
    ],
    "authors": [{
        "name": "Emmanuel Belair",
        "email": "contact@ebelair.fr"
    }],
    "minimum-stability" : "dev",
    "config": {
        "process-timeout": 5000
    },
    "require" : {
        "php" : ">=5.5",
        "zendframework/zendframework" : "2.*"
    },
    "require-dev" : {
        "doctrine/doctrine-orm-module": "0.*",
        "satooshi/php-coveralls": "dev-master",
        "phpunit/phpunit" : "*"
    },
    "autoload" : {
        "psr-0" : {
            "SuchTable\\" : "src/"
        }
    }
}
